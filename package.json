  "version": "0.3.2",
  "author": "Omar Parkes <omr.prks@gmail.com> (https://github.com/omrprks)",
  "publisher": "omrprks",
  "license": "MIT",
  "repository": "https://github.com/omrprks/vscode-theme-rhubarb.git",
